<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Test Suite" thread-count="1" verbose="1">
    <parameter name="platform" value="android2"/>
    <test name="Smoke Tests">
        <groups>
            <run>
                <include name="smoke android2"/>
            </run>
        </groups>
        <classes>
            <class name="tests.MainTest"/>
<!--            <class name="tests.ProfileTest"/>-->
        </classes>
    </test>
</suite>
